          <h1 class="subTitle"><%= resName%></h1><!--Name of record / result-->

          <% for(x in result){ %>
              <% if(x==="id"){ continue; } //Skip ID field %>
              <% if(result[x]=="nan" || result[x]=="" || result[x]==null){ continue; } //Skip empty fields %>
              
              <p class="subHead"> <%= sub[x] %> </p><!--Field Header-->
      
              <% if(x==="gene_name"){ %>
                  <pre style="font-style:italic;"  class="content" name="content"><%= result[x] %></pre>
              <% } else if(x==='structure_2d'){ %>
                <img src="/images/testCompound2d/<%= result[x] %>" alt="2D Structure of <%= result['test_compound'] %>"/>
              
              <% } else { %>
              <pre  class="content" name="content" id="<%= x %>"><%- result[x]%></pre>
              <% } %>
              <hr>
          <% } %>
      
          <script src="/javascripts/resultsPage.js"></script>

